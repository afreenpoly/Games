<%- include('header.ejs') %>

    <div class="box" id="heading">
      <h1><%= day %></h1>
    </div>
    <div class="box">
        <% for (var i=0; i<items.length; i++) { %>
        <div class="item">
          <input type="checkbox">
            <p><%= items[i] %></p>
        </div>
        <% } %>

      <form class="item" action="/" method="post">
        <input type="text" name="newitem" placeholder="Enter New Item" autocomplete="off">
        <button type="submit" name="submit">+</button>
      </form>
    </div>
  </body>
</html>
